{
  "expo": {
    "name": "harsh-yadav-chatbot",
    "slug": "harsh-yadav-chatbot",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/favicon.webp",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "name": "Harsh's Voice Chatbot",
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.webp"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "android": {
      "package": "com.anonymous.boltexponativewind"
    }
  }
}
